{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["ES6", "DOM"],
      "types": ["cypress", "@badeball/cypress-cucumber-preprocessor"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["cypress/e2e/**/*.ts", "cypress/support/**/*.ts"]
  }
  